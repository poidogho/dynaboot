{
  "name": "dynaboot",
  "version": "1.0.0",
  "description": "Express server",
  "main": "server.ts",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "export NODE_ENV=test && npm run lint && jest --verbose --no-cache",
    "test-coverage": "export NODE_ENV=test && npm run lint && jest --verbose --no-cache --coverage",
    "start:build": "tsc -w",
    "start:run": "nodemon dist/server.js",
    "start": "concurrently npm:start:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poidogho/dynaboot.git"
  },
  "author": "Odafe Idogho",
