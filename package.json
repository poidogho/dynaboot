  "bugs": {
    "url": "https://github.com/poidogho/dynaboot/issues"
  },
  "homepage": "https://github.com/poidogho/dynaboot#readme",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.13"
  },
  "devDependencies": {
    "@types/bson": "^4.2.0",
    "typescript": "^4.5.2"
  }
}
